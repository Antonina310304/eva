    "@u-wave/react-vimeo": "^0.9.0",
    "abortcontroller-polyfill": "^1.4.0",
    "classnames": "^2.3.1",
    "core-js": "^3.6.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.6",
    "intersection-observer": "^0.10.0",
