{
  "name": "react-render",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "rm -rf node_modules/.cache/webpack",
    "build": "NODE_ENV=production npm run compile && NODE_ENV=production webpack --config webpack/configs/main.ts",
    "prod": "npm run build && npm run start",
    "dev": "NODE_ENV=development nodemon server/index.dev.ts",
    "compile": "tsc server/index.ts --jsx react --skipLibCheck --esModuleInterop --outDir dist",
    "start": "NODE_ENV=production node dist/server/index.js",
    "analyzer:node": "mkdir -p webpack/stats && NODE_ENV=production webpack --config webpack/configs/node.ts --profile --json > webpack/stats/node.json && webpack-bundle-analyzer webpack/stats/node.json dist/node",
    "analyzer:web": "mkdir -p webpack/stats && NODE_ENV=production webpack --config webpack/configs/web.ts --profile --json > webpack/stats/web.json && webpack-bundle-analyzer webpack/stats/web.json dist/web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "is-ci || husky install",
    "lint:all": "npm run lint:js && npm run lint:css",
    "lint:all:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint './' --ext '.js, .jsx, .ts, .tsx'",
    "lint:js:fix": "eslint './' --ext '.js, .jsx, .ts, .tsx' --fix",
    "lint:css": "stylelint **.css",
    "lint:css:fix": "stylelint **.css --fix",
    "code-style": "prettier --check .",
    "code-style:fix": "prettier --write .",
    "type-check": "eslint --ext '.js, .jsx, .ts, .tsx' ./client/* ./server/* ./webpack/*"
  },
  "author": "Yury Kundin",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.10.4",
    "@divanru/icons": "^1.59.0",
